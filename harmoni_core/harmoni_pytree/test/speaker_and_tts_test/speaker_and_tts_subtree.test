<launch>
    <include file="$(find audio_play)/launch/play.launch">
        <arg name="do_timestamp" value = "false"/> 
        <arg name="format" value = "wave"/>
        <arg name="sample_rate" value="22050"/>
    </include>
    
    <include file="$(find harmoni_speaker)/launch/speaker_service.launch"></include>
    
    <include file="$(find harmoni_tts)/launch/tts_service.launch"></include>

    <param name="test_speaker_input" value="$(find harmoni_tts)/temp_data/tts.wav"/>
    <param name="test_polly_input" value="{'message':'I would like to develop harmoni'}"/>
    <test test-name="test_speaker_and_tts" pkg="harmoni_pytree" type="speaker_and_tts_test.py"/>
</launch>